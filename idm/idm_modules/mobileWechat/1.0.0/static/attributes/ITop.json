                    "type": "colorPicker",
                    "layoutType": "inline",
                    "text": "文字颜色",
                    "bindKey": "mainMinorColor",
                    "desc": "应用于文字颜色",
                    "placeholder": "请选择颜色",
                    "display": "@[mainKey]"
                },
