                {
                    "type": "inputNumberUnit",
                    "layoutType": "inline",
                    "text": "纵向偏移",
                    "bindKey": "mainPositionY",
                    "dictionary": [
                        {
                            "label": "px",
                            "value": "px"
                        },
                        {
                            "label": "%",
                            "value": "%"
                        },
                        {
                            "label": "em",
                            "value": "em"
                        }
