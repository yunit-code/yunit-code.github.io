                        {
                            "type": "inputNumberUnit",
                            "layoutType": "inline",
                            "text": "纵向偏移",
                            "bindKey": "positionYError",
                            "dictionary": [
                                {
                                    "label": "px",
                                    "value": "px"
                                },
                                {
                                    "label": "%",
                                    "value": "%"
                                },
                                {
                                    "label": "em",
                                    "value": "em"
                                }
                            ],
                            "display": "@[bgImgUrlError]"
                        },
                        {
                            "type": "radio",
                            "layoutType": "block",
                            "text": "背景大小",
                            "bindKey": "bgSizeError",
                            "dictionary": [
                                {
                                    "label": "裁切显示",
                                    "value": "cover"
                                },
                                {
                                    "label": "完全显示",
                                    "value": "contain"
                                },
                                {
                                    "label": "自定义",
                                    "value": "custom"
                                }
                            ],
                            "display": "@[bgImgUrlError]"
                        },
                        {
                            "type": "inputNumberUnit",
                            "layoutType": "inline",
                            "text": "宽度",
                            "bindKey": "bgSizeWidthError",
                            "dictionary": [
                                {
                                    "label": "px",
                                    "value": "px"
                                },
                                {
                                    "label": "%",
                                    "value": "%"
                                },
                                {
                                    "label": "em",
                                    "value": "em"
                                }
                            ],
                            "display": "@[bgSizeError=='custom']"
                        },
                        {
                            "type": "inputNumberUnit",
                            "layoutType": "inline",
                            "text": "高度",
                            "bindKey": "bgSizeHeightError",
                            "dictionary": [
                                {
                                    "label": "px",
                                    "value": "px"
                                },
                                {
                                    "label": "%",
                                    "value": "%"
                                },
                                {
                                    "label": "em",
                                    "value": "em"
                                }
                            ],
                            "display": "@[bgSizeError=='custom']"
                        },
                        {
                            "type": "radio",
                            "layoutType": "block",
                            "text": "平铺模式",
                            "bindKey": "bgRepeatError",
                            "dictionary": [
                                {
                                    "label": "双向重复",
                                    "value": "repeat"
                                },
                                {
                                    "label": "水平重复",
                                    "value": "repeat-x"
                                },
                                {
                                    "label": "垂直重复",
                                    "value": "repeat-y"
                                },
                                {
                                    "label": "不重复",
                                    "value": "no-repeat"
                                },
                                {
                                    "label": "继承",
                                    "value": "inherit"
                                }
                            ],
                            "display": "@[bgImgUrlError]"
                        },
                        {
                            "type": "radio",
                            "layoutType": "inline",
                            "text": "背景模式",
                            "bindKey": "bgAttachmentError",
                            "dictionary": [
                                {
                                    "label": "固定",
                                    "value": "fixed"
                                },
                                {
                                    "label": "滚动",
                                    "value": "scroll"
                                },
                                {
                                    "label": "继承",
                                    "value": "inherit"
                                }
                            ],
                            "display": "@[bgImgUrlError]"
                        }
                    ]
                }
            ]
        },
        {
            "type": "group",
            "text": "高级",
            "desc": "点击？查看高级设置用法指南",
            "children": [
                {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "表单标识",
                    "bindKey": "formFiledKey",
                    "desc": "如果为空则会提交表单时用唯一标识作为key",
                    "default": ""
                },
                {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "分组标识",
                    "bindKey": "formGroupKey",
                    "desc": "表单分组标识，用于批量提交分组标识相同的表单控件值，减少提交控件一个一个选择控件的范围",
                    "default": "formGroupKey"
                },
                {
                    "type":"group",
                    "text":"回显与状态",
                    "desc":"用于回显控件的结果值与控件的状态控制，共四种状态：（编辑：default）、（禁用：disabled）、（只读：readonly）、（隐藏：hidden）",
                    "children":[
                        {
                            "type": "input",
                            "layoutType": "inline",
                            "text": "结果集名",
                            "bindKey": "echoDataName",
                            "desc": "页面接口设定的结果集名称，位置为：页面设置》高级设置》页面接口",
                            "default": "",
                            "placeholder": "示例：resultData"
                        },
                        {
                            "type": "select",
                            "layoutType": "inline",
                            "text": "回显方式",
                            "bindKey": "echoType",
                            "dictionary": [
                                {
                                    "label": "同表单标识",
                                    "value": "equalFormKey"
                                },
                                {
                                    "label": "自定义字段",
                                    "value": "customFiled"
                                },
                                {
                                    "label": "自定义函数",
                                    "value": "customFunction"
                                }
                            ],
                            "default": "equalFormKey"
                        },
                        {
                            "type": "textarea",
                            "layoutType": "inline",
                            "text": "回显字段",
                            "bindKey": "echoDataFiled",
                            "desc": "根据接口返回数据格式指定结果集的字段，比如结果集名为resultData且它的值为{data:{filedName:'1000'}}，则这里应该填写data.filedName",
                            "default": "",
                            "placeholder": "示例：data.filedName 或者 [0].data[0].filedName",
                            "display":"@[echoType=='customFiled']"
                        },
                        {
                            "type": "actionSelect",
                            "layoutType": "block",
                            "text": "回显自定义函数",
                            "bindKey": "echoFunction",
                            "desc": "回显值时会调用此处设置的方法返回控件需要的回显值，接收参数格式为{...自定义的,...结果集名对象}",
                            "multiple": false,
                            "display":"@[echoType=='customFunction']"
                        },
                        {
                            "type": "textarea",
                            "layoutType": "inline",
                            "text": "控件状态",
                            "bindKey": "stateDataFiled",
                            "desc": "主要用于控制控件的显示状态。根据接口返回数据格式指定结果集的字段，比如结果集名为resultData且它的值为{data:{filedName:'1000'}}，则这里应该填写data.filedName，值必须为指定的四种（编辑：default）、（禁用：disabled）、（只读：readonly）、（隐藏：hidden）",
                            "default": "",
                            "placeholder": "示例：data.filedName 或者 [0].data[0].filedName"
                        },
                        {
                            "type": "actionSelect",
                            "layoutType": "block",
                            "text": "控件状态自定义函数",
                            "bindKey": "stateFunction",
                            "desc": "要用于控制控件的显示状态。加载时会调用此处设置的方法返回控件需要的控件状态值，接收参数格式为{...自定义的,...结果集名对象}，返回值必须为指定的四种（编辑：default）、（禁用：disabled）、（只读：readonly）、（隐藏：hidden）",
                            "multiple": false
                        }
                    ]
                }
            ]
        }
    ]
}