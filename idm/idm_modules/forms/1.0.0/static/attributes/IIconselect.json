{
    "classId": "idm.componet.forms.iiconselect",
    "comName": "图标选择",
    "className": "IIconselect",
    "comType": "common",
    "comLangue": "vue",
    "compositeAttr": [
        {
            "type": "input",
            "layoutType": "inline",
            "text": "唯一标识",
            "bindKey": "ctrlId",
            "disabled": true,
            "default": "@[packageid]",
            "desc": "",
            "helpUrl": ""
        },
        {
            "type": "group",
            "text": "基本属性",
            "desc": "提供设置组件能达到展示效果的基本属性设置信息",
            "children": [
                {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "标题",
                    "bindKey": "label",
                    "default": "字体图标"
                },
                {
                    "type": "switch",
                    "layoutType": "inline",
                    "text": "显示标题",
                    "bindKey": "labelDisplay",
                    "default": true
                },
                {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "提示文字",
                    "bindKey": "placeholder",
                    "placeholder": "请输入"
                },
                {
                    "type": "textarea",
                    "layoutType": "inline",
                    "text": "默认值",
                    "bindKey": "defaultValue",
                    "default": ""
                },
                {
                    "type": "inputNumberUnit",
                    "layoutType": "inline",
                    "text": "最大宽度",
                    "bindKey": "maxWidth",
                    "dictionary": [
                        {
                            "label": "auto",
                            "value": "auto"
                        },
                        {
                            "label": "px",
                            "value": "px"
                        },
                        {
                            "label": "%",
                            "value": "%"
                        },
                        {
                            "label": "rem",
                            "value": "rem"
                        },
                        {
                            "label": "em",
                            "value": "em"
                        }
                    ],
                    "default": 0
                },
                {
                    "type": "inputNumberUnit",
                    "layoutType": "inline",
                    "text": "最小宽度",
                    "bindKey": "minWidth",
                    "dictionary": [
                        {
                            "label": "auto",
                            "value": "auto"
                        },
                        {
                            "label": "px",
                            "value": "px"
                        },
                        {
                            "label": "%",
                            "value": "%"
                        },
                        {
                            "label": "rem",
                            "value": "rem"
                        },
                        {
                            "label": "em",
                            "value": "em"
                        }
                    ],
                    "default": 0
                },
                {
                    "type": "inputNumberUnit",
                    "layoutType": "inline",
                    "text": "右边距",
                    "bindKey": "rightPaddingNumber",
                    "dictionary": [
                        {
                            "label": "auto",
                            "value": "auto"
                        },
                        {
                            "label": "px",
                            "value": "px"
                        },
                        {
                            "label": "%",
                            "value": "%"
                        },
                        {
                            "label": "rem",
                            "value": "rem"
                        },
                        {
                            "label": "em",
                            "value": "em"
                        }
                    ],
                    "default": 0
                },
                {
                    "type": "radio",
                    "layoutType": "inline",
                    "text": "标题排列",
                    "bindKey": "labelLayout",
                    "dictionary": [
                        {
                            "label": "横向排列",
                            "value": "horizontal"
                        },
                        {
                            "label": "纵向排列",
                            "value": "vertical"
                        }
                    ],
                    "default": "horizontal"
                },
                {
                    "type": "radio",
                    "layoutType": "inline",
                    "text": "尺寸",
                    "bindKey": "size",
                    "dictionary": [
                        {
                            "label": "小",
                            "value": "small"
                        },
                        {
                            "label": "中",
                            "value": "default"
                        },
                        {
                            "label": "大",
                            "value": "large"
                        }
                    ],
                    "default": "default"
                },
                {
                    "type": "radio",
                    "layoutType": "inline",
                    "text": "栅格布局",
                    "bindKey": "gridLayout",
                    "dictionary": [
                        {
                            "label": "无",
                            "value": "close"
                        },
                        {
                            "label": "6:12",
                            "value": "6:12"
                        },
                        {
                            "label": "6:18",
                            "value": "6:18"
                        },
                        {
                            "label": "12:12",
                            "value": "12:12"
                        }
                    ],
                    "default": "close",
                    "display": "@[labelLayout=='horizontal']"
                },
                {
                    "type": "radio",
                    "layoutType": "inline",
                    "text": "默认状态",
                    "bindKey": "defaultStatus",
                    "dictionary": [
                        {
                            "label": "普通",
                            "value": "default"
                        },
                        {
                            "label": "禁用",
                            "value": "disabled"
                        },
                        {
                            "label": "只读",
                            "value": "readonly"
                        },
                        {
                            "label": "隐藏",
                            "value": "hidden"
                        }
                    ],
                    "default": "default"
                },
                {
                    "type": "richText",
                    "layoutType": "inline",
                    "text": "描述内容",
                    "bindKey": "descContent"
                },
                {
                    "type": "radio",
                    "layoutType": "inline",
                    "text": "描述位置",
                    "bindKey": "descPosition",
                    "dictionary": [
                        {
                            "label": "横向",
                            "value": "horizontal"
                        },
                        {
                            "label": "纵向",
                            "value": "vertical"
                        }
                    ],
                    "default": "vertical",
                    "display": "@[gridLayout=='6:12']"
                },
                {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "图标URL",
                    "bindKey": "iconfonturl",
                    "desc": "从iconfont下载的图标库文件夹地址，不需要具体到文件，比如：图标css文件的路径是【/project/font_1248060_zpcega7i6m9/iconfont.css】，我们这里只需要填写【/project/font_1248060_zpcega7i6m9/】，注意：里面的文件建议不要做任何修改，否则可能读取不正确",
                    "placeholder": "请输入"
                },
                {
                    "type": "switch",
                    "layoutType": "inline",
                    "text": "是否多选",
                    "bindKey": "multipleSelect",
                    "default": false
                },
                {
                    "type": "inputNumber",
                    "layoutType": "inline",
                    "text": "多选数量",
                    "bindKey": "multipleMaxCount",
                    "desc": "多选情况下最多不能超过的数量",
                    "placeholder": "请输入",
                    "display": "@[multipleSelect]"
                },
                {
                    "type": "switch",
                    "layoutType": "inline",
                    "text": "校验占位",
                    "desc":"是否预留校验信息空间，开启将会在底部占用25px空白预留给校验信息使用",
                    "bindKey": "retainBottomHeight",
                    "default": true
                }
            ]
        },
        {
            "type": "group",
            "text": "校验",
            "desc": "提供校验输入内容格式设置",
            "children": [
                {
                    "type": "switch",
                    "layoutType": "inline",
                    "text": "必填",
                    "bindKey": "required"
                },
                {
                    "type": "textarea",
                    "layoutType": "block",
                    "text": "必填错误提示",
                    "bindKey": "requiredText",
                    "default": "",
                    "display": "@[required]"
                },
                {
                    "type": "switch",
                    "layoutType": "inline",
                    "text": "自定义函数",
                    "bindKey": "customFun"
                },
                {
                    "type": "textarea",
                    "layoutType": "block",
                    "text": "函数代码",
                    "bindKey": "customFunCode",
                    "display": "@[customFun]",
                    "default": "function(value){return true}"
                },
                {
                    "type": "textarea",
                    "layoutType": "block",
                    "text": "自定义函数错误提示",
                    "bindKey": "customFunText",
                    "default": "",
                    "display": "@[customFun]"
                }
            ]
        },
        {
            "type": "group",
            "text": "样式设置",
            "desc": "点击？查看样式设置用法指南",
            "helpUrl": "",
            "children": [
                {
                    "type": "font",
                    "layoutType": "block",
                    "text": "标题文字",
                    "bindKey": "labelFont"
                },
                {
                    "type": "font",
                    "layoutType": "block",
                    "text": "错误提示文字",
                    "bindKey": "errorMsgFont"
                },
                {
                    "type": "font",
                    "layoutType": "block",
                    "text": "按钮文字",
                    "bindKey": "contentBtnFont",
                    "desc": "编辑状态下的按钮文字样式设置"
                },
                {
                    "type": "colorPicker",
                    "layoutType": "inline",
                    "text": "主题颜色",
                    "bindKey": "mainColor",
                    "placeholder": "请选择",
                    "desc": "弹出层选择图标的主题颜色",
                    "default": {}
                },
                {
                    "type": "font",
                    "layoutType": "block",
                    "text": "内容图标（编辑）",
                    "bindKey": "contentEditFont",
                    "desc": "编辑状态下的图标样式设置"
                },
                {
                    "type": "font",
                    "layoutType": "block",
                    "text": "内容图标（只读）",
                    "bindKey": "contentReadFont",
                    "desc": "只读情况下的图标样式设置"
                },
                {
                    "type": "group",
                    "text": "默认状态样式",
                    "desc": "默认情况下的样式设置",
                    "helpUrl": "",
                    "children": [
                        {
                            "type": "box",
                            "layoutType": "block",
                            "text": "内外边距",
                            "bindKey": "box"
                        },
                        {
                            "type": "border",
