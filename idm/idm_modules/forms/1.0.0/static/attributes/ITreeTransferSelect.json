                        {
                            "label": "auto",
                            "value": "auto"
                        },
                        {
                            "label": "px",
                            "value": "px"
                        },
                        {
                            "label": "%",
                            "value": "%"
                        },
                        {
                            "label": "rem",
                            "value": "rem"
                        },
                        {
                            "label": "em",
                            "value": "em"
                        }
