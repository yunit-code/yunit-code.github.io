                        {
                            "label": "只显示子节点",
                            "value": "SHOW_CHILD"
                        },
                        {
                            "label": "只显示父节点",
                            "value": "SHOW_PARENT"
                        },
                        {
                            "label": "所有选中节点",
                            "value": "SHOW_ALL"
                        }
                    ],
                    "styleType":"radio",
                    "default": "SHOW_CHILD"
                },
