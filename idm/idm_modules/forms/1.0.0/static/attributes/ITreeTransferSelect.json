                        {
                            "label": "普通",
                            "value": "default"
                        },
                        {
                            "label": "禁用",
                            "value": "disabled"
                        },
                        {
                            "label": "只读",
                            "value": "readonly"
                        },
                        {
                            "label": "隐藏",
                            "value": "hidden"
                        }
