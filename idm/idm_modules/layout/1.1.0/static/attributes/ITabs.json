                                    "value": "px"
                                },
                                {
                                    "label": "%",
                                    "value": "%"
                                },
                                {
                                    "label": "em",
                                    "value": "em"
                                }
                            ],
                            "display": "@[bgImgUrl]"
                        },
                        {
