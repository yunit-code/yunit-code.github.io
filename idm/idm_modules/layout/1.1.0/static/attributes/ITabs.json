{
    "classId": "idm.componet.layout.itabs",
    "comName": "选项卡",
    "className": "ITabs",
    "comType": "common",
    "comLangue": "vue",
    "compositeAttr": [
        {
            "type": "input",
            "layoutType": "inline",
            "text": "唯一标识",
            "bindKey": "ctrlId",
            "disabled": true,
            "default": "@[packageid]",
            "desc": "组件的唯一标识，自动生成的，请勿直接在任何地方直接写死，以防数据发生变化而导致功能失效"
        },{
            "type": "group",
            "text": "基本属性",
            "desc": "提供设置组件能达到展示效果的基本属性设置信息",
            "children": [
                {
                    "type": "radio",
                    "layoutType": "inline",
                    "text": "页签大小",
                    "bindKey": "size",
                    "dictionary": [
                        {
                            "label": "小",
                            "value": "small"
                        },
                        {
                            "label": "中",
                            "value": "default"
                        },
                        {
                            "label": "大",
                            "value": "large"
                        }
                    ],
                    "default": "default"
                },
                {
                    "type": "radio",
                    "layoutType": "inline",
                    "text": "页签位置",
                    "bindKey": "tabPosition",
                    "dictionary": [
                        {
                            "label": "top",
                            "value": "top"
                        },
                        {
                            "label": "right",
                            "value": "right"
                        },
                        {
                            "label": "bottom",
                            "value": "bottom"
                        },
                        {
                            "label": "left",
                            "value": "left"
                        }
                    ],
                    "default": "top"
                },
                {
                    "type": "inputNumber",
                    "layoutType": "inline",
                    "text": "页签间距",
                    "bindKey": "tabBarGutter",
                    "maxLength": 999,
                    "minLength": 0
                },
                {
                    "type": "radio",
                    "layoutType": "inline",
                    "text": "风格样式",
                    "bindKey": "type",
                    "dictionary": [
                        {
                            "label": "line",
                            "value": "line"
                        },
                        {
                            "label": "card",
                            "value": "card"
                        }
                    ],
                    "default": "line"
                },
                {
                    "type": "switch",
                    "layoutType": "inline",
                    "text": "切换动画",
                    "desc":"是否使用动画切换 Tabs，在 页签位置=top|bottom 时有效",
                    "bindKey": "animated",
                    "default": true
                }
            ]
        },
        {
            "type": "table",
            "text": "页签选项设置",
            "bindKey": "TabPaneList",
            "tabText": "页签[index]",
            "children": [
                {
                    "type": "input",
                    "layoutType": "inline",
                    "text": "tab标识",
                    "bindKey": "key"
                },{
                    "type": "input",
                    "layoutType": "inline",
                    "text": "tab文字",
                    "bindKey": "tab"
                },
                {
                    "type": "actionSelect",
                    "layoutType": "block",
                    "text": "tab显示自定义函数",
                    "bindKey": "tabSlotFunction",
                    "desc": "tab页签内容自定义函数，支持返回HTML格式字符串，接收参数为格式为{...自定义的,当前页签数据对象}，返回html字符串",
                    "multiple": false
                },
                {
                    "type": "switch",
                    "layoutType": "inline",
                    "text": "默认选中",
                    "bindKey": "defaultActiveKey",
                    "default": false
                },
                {
                    "type": "switch",
                    "layoutType": "inline",
                    "text": "懒加载",
                    "bindKey": "forceRender",
                    "desc":"选用此项则被隐藏的tab默认不渲染dom，点击的时候第一次去渲染tab",
                    "default": false
                }
            ]
        },
        {
            "type": "group",
            "text": "悬浮容器属性",
            "desc": "提供设置组件的悬浮容器属性设置信息",
            "children": [
              {
                "type": "switch",
                "layoutType": "inline",
                "text": "悬浮容器",
                "desc": "是否使用悬浮容器",
                "bindKey": "showDragContainer",
                "default": false
              },
              {
                "type": "input",
                "layoutType": "inline",
                "text": "坐标(X)",
                "desc": "对应left属性，以组件左上角为起点，可填%，px",
                "bindKey": "dragContainerX",
                "default": "60%",
                "display":"@[showDragContainer]"
              },
              {
                "type": "input",
                "layoutType": "inline",
                "text": "坐标(Y)",
                "desc": "对应top属性，以组件左上角为起点，可填%，px",
                "bindKey": "dragContainerY",
                "default": "8px",
                "display":"@[showDragContainer]"
              },
              {
                "type": "select",
                "layoutType": "inline",
                "text": "显示方式",
                "bindKey": "dragContainerShowType",
                "desc": "可以根据切换显示方式对悬浮容器进行更高级的控制，更多使用方法请点击参考更详细的说明",
                "dictionary": [
                  {
                    "label": "始终显示",
                    "value": "default"
                  },
                  {
                    "label": "切换依据显示",
                    "value": "toggle"
                  },
                  {
                    "label": "自定义函数显示",
                    "value": "custom"
                  }
                ],
                "default": "default",
                "display":"@[showDragContainer]"
              },
              {
                "type": "input",
                "layoutType": "inline",
                "text": "显示依据",
                "desc": "当显示方式为“切换依据显示”的时候代表页签选项卡切换的时候会根据当前填写的依据字段来去匹配结果值，为true则显示，false则不显示，此处填写表达式，更多详细用法点击查看",
                "bindKey": "dragContainerDataFiled",
                "display": "@[dragContainerShowType=='toggle'&&showDragContainer]",
                "placeholder": "示例：moreUrl 或者 [0].showSetting 或者 key==1(页签tab标识，而非索引)"
              },
              {
                "type": "actionSelect",
                "layoutType": "block",
                "text": "自定义函数依据显示",
                "bindKey": "dragContainerDataFunction",
                "display": "@[dragContainerShowType=='custom'&&showDragContainer]",
                "desc": "切换选项卡、数据勾选等动作会触发回调来控制是否显示，动态属性模式下也会调用此函数，接收参数为格式为{...url参数,customParam:自定义的,activeKey,allKey,actionType:动作类型,otherData}",
                "multiple": false
              }
            ]
        },
        {
            "type": "group",
            "text": "样式设置",
            "desc": "点击？查看样式设置用法指南",
            "helpUrl": "",
            "children": [
                {
                    "type": "box",
                    "layoutType": "block",
                    "text": "内外边距",
                    "bindKey": "box"
                },
                {
                    "type": "inlineGroup",
                    "text": "宽高",
                    "children": [
                        {
                            "type": "input",
                            "layoutType": "block",
                            "text": "宽",
                            "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
                            "bindKey": "width",
                            "width": "60px",
                            "default": "auto"
                        },
                        {
                            "type": "input",
                            "layoutType": "block",
                            "text": "高",
                            "desc": "填写auto则为自适应，或者使用px、%、vh等单位，比如100%、100px、100vh等等",
                            "bindKey": "height",
                            "width": "60px",
                            "default": "auto"
                        }
                    ]
                },
                {
                    "type": "inlineGroup",
                    "text": "最小宽高",
                    "children": [
                        {
                            "type": "input",
                            "layoutType": "block",
                            "text": "最小宽",
                            "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
                            "bindKey": "minWidth",
                            "width": "60px",
                            "default": "auto"
                        },
                        {
                            "type": "input",
                            "layoutType": "block",
                            "text": "最小高",
                            "desc": "填写auto则为自适应，或者使用px、%、vh等单位，比如100%、100px、100vh等等",
                            "bindKey": "minHeight",
                            "width": "60px",
                            "default": "auto"
                        }
                    ]
                },
                {
                    "type": "group",
                    "text": "背景设置",
                    "desc": "点击？查看背景设置用法指南",
                    "helpUrl": "",
                    "children": [
                        {
                            "type": "colorPicker",
                            "layoutType": "inline",
                            "text": "背景色",
                            "bindKey": "bgColor",
                            "placeholder": "请选择颜色",
                            "default": {}
                        },
                        {
                            "type": "uploadImage",
                            "layoutType": "inline",
                            "text": "背景图片",
                            "bindKey": "bgImgUrl",
                            "placeholder": "可输入图片地址或直接上传"
                        },
                        {
                            "type": "inputNumberUnit",
                            "layoutType": "inline",
                            "text": "横向偏移",
                            "bindKey": "positionX",
                            "dictionary": [
                                {
                                    "label": "px",
                                    "value": "px"
                                },
                                {
                                    "label": "%",
                                    "value": "%"
                                },
                                {
                                    "label": "em",
                                    "value": "em"
                                }
                            ],
                            "display": "@[bgImgUrl]"
                        },
                        {
                            "type": "inputNumberUnit",
                            "layoutType": "inline",
                            "text": "纵向偏移",
